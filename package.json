{
  "name": "slex-store-worker",
  "version": "1.0.0",
  "repository": "https://github.com/alexstroukov/slex-store-worker",
  "license": "MIT",
  "main": "compiled/slexStoreWorker.js",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "class-property"
    ],
    "ignore": [
      "node_modules/**"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "fetch",
      "alert",
      "localStorage",
      "btoa",
      "atob"
    ]
  },
  "scripts": {
    "mocha": "mocha",
    "watch": "webpack --config webpack/webpack.config.js --watch",
    "build": "webpack --config webpack/webpack.config.js",
    "test": "mocha compiled/test",
    "clean": "rm -rf compiled",
    "deep-clean": "rm -rf node_modules && npm run clean",
    "prepublishOnly": "rm -rf compiled && webpack --config webpack/src.webpack.config.js"
  },
  "dependencies": {
    "deep-diff": "0.3.8",
    "lodash": "4.17.13"
  },
  "devDependencies": {
    "slex-store": "^1.0.0",
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "1.4.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-runtime": "6.26.0",
    "babel-eslint": "7.2.3",
    "eslint-plugin-class-property": "1.0.6",
    "chai": "4.1.1",
    "mocha": "3.5.0",
    "sinon": "3.2.1",
    "webpack": "3.5.5",
    "webpack-node-externals": "1.6.0"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  }
}
